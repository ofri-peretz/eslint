{
    "compilerOptions": {
        "module": "CommonJS",
        "target": "ES2020",
        "lib": [
            "ES2020"
        ],
        "strict": false,
        "esModuleInterop": true,
        "skipLibCheck": true,
        "allowJs": true,
        "checkJs": false,
        "noEmit": true,
        "resolveJsonModule": true,
        "moduleResolution": "node"
    },
    "include": [
        "**/*.js"
    ],
    "exclude": [
        "node_modules"
    ]
}