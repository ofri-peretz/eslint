{
  "name": "eslint-plugin-jwt",
  "version": "2.1.1",
  "description": "Security-focused ESLint plugin for JWT operations. Detects algorithm confusion (CVE-2022-23540), weak secrets, missing validation, and library-specific vulnerabilities across jsonwebtoken, jose, express-jwt, @nestjs/jwt, jwks-rsa, and jwt-decode.",
  "type": "commonjs",
  "main": "./src/index.js",
  "types": "./src/index.d.ts",
  "exports": {
    ".": {
      "types": "./src/index.d.ts",
      "default": "./src/index.js"
    },
    "./types": {
      "types": "./src/types/index.d.ts",
      "default": "./src/types/index.js"
    }
  },
  "author": "Ofri Peretz <ofriperetzdev@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/ofri-peretz/eslint/blob/main/packages/eslint-plugin-jwt/README.md",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ofri-peretz/eslint.git",
    "directory": "packages/eslint-plugin-jwt"
  },
  "bugs": {
    "url": "https://github.com/ofri-peretz/eslint/issues"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "src/",
    "dist/",
    "README.md",
    "LICENSE",
    "CHANGELOG.md",
    "AGENTS.md"
  ],
  "keywords": [
    "eslint",
    "eslint-plugin",
    "eslintplugin",
    "jwt",
    "jsonwebtoken",
    "jose",
    "express-jwt",
    "nestjs-jwt",
    "jwks-rsa",
    "jwt-decode",
    "security",
    "owasp",
    "cwe-347",
    "cwe-757",
    "algorithm-confusion",
    "cve-2022-23540",
    "token",
    "authentication",
    "authorization",
    "llm-optimized",
    "ai-assistant",
    "auto-fix",
    "typescript",
    "static-analysis"
  ],
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "@interlace/eslint-devkit": "^1.2.1"
  },
  "peerDependencies": {},
  "peerDependenciesMeta": {
    "jsonwebtoken": {
      "optional": true
    },
    "@nestjs/jwt": {
      "optional": true
    },
    "express-jwt": {
      "optional": true
    },
    "jose": {
      "optional": true
    },
    "jwks-rsa": {
      "optional": true
    },
    "jwt-decode": {
      "optional": true
    }
  },
  "devDependencies": {
    "@typescript-eslint/parser": "^8.46.2",
    "@typescript-eslint/rule-tester": "^8.46.2"
  }
}
