{
  "name": "eslint-plugin-jwt",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/eslint-plugin-jwt/src",
  "projectType": "library",
  "tags": [],
  "targets": {
    "build": {
      "executor": "@nx/js:tsc",
      "outputs": [
        "{options.outputPath}"
      ],
      "options": {
        "outputPath": "dist/packages/eslint-plugin-jwt",
        "main": "packages/eslint-plugin-jwt/src/index.ts",
        "tsConfig": "packages/eslint-plugin-jwt/tsconfig.lib.json",
        "assets": [
          "packages/eslint-plugin-jwt/*.md",
          "packages/eslint-plugin-jwt/.npmignore"
        ]
      }
    },
    "nx-release-publish": {
      "executor": "@nx/js:release-publish",
      "options": {
        "packageRoot": "dist/{projectRoot}"
      }
    }
  }
}
