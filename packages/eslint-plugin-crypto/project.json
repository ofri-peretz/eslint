{
  "name": "eslint-plugin-crypto",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/eslint-plugin-crypto/src",
  "projectType": "library",
  "tags": [],
  "targets": {
    "build": {
      "executor": "@nx/js:tsc",
      "outputs": [
        "{options.outputPath}"
      ],
      "options": {
        "outputPath": "dist/packages/eslint-plugin-crypto",
        "main": "packages/eslint-plugin-crypto/src/index.ts",
        "tsConfig": "packages/eslint-plugin-crypto/tsconfig.lib.json",
        "assets": [
          "packages/eslint-plugin-crypto/*.md",
          "packages/eslint-plugin-crypto/.npmignore"
        ]
      }
    },
    "nx-release-publish": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npm publish dist/{projectRoot} --access public --auth-type=web",
        "cwd": "{workspaceRoot}"
      }
    }
  }
}
