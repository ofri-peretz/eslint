{
  "name": "@interlace/eslint-devkit",
  "version": "1.3.1",
  "description": "TypeScript utilities for building LLM-optimized ESLint plugins - AST helpers, type utilities, security benchmarks, and SARIF output",
  "type": "commonjs",
  "main": "./src/index.js",
  "types": "./src/index.d.ts",
  "exports": {
    ".": {
      "types": "./src/index.d.ts",
      "require": "./src/index.js",
      "import": "./src/index.js"
    },
    "./resolver": {
      "types": "./src/resolver.d.ts",
      "require": "./src/resolver.js",
      "import": "./src/resolver.js"
    },
    "./package.json": "./package.json"
  },
  "author": "Ofri Peretz <ofriperetzdev@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/ofri-peretz/eslint#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ofri-peretz/eslint.git",
    "directory": "packages/eslint-devkit"
  },
  "bugs": {
    "url": "https://github.com/ofri-peretz/eslint/issues"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "src/",
    "dist/",
    "README.md",
    "LICENSE",
    "CHANGELOG.md",
    "AGENTS.md"
  ],
  "keywords": [
    "eslint",
    "eslint-devkit",
    "eslint-toolkit",
    "typescript",
    "utilities",
    "ast",
    "type-checking",
    "plugin-development",
    "llm-optimized",
    "ai-assistant",
    "auto-fix",
    "sarif",
    "owasp",
    "cwe",
    "security"
  ],
  "engines": {
    "node": ">=18.0.0"
  },
  "peerDependencies": {},
  "peerDependenciesMeta": {
    "get-tsconfig": {
      "optional": true
    },
    "enhanced-resolve": {
      "optional": true
    }
  },
  "optionalDependencies": {},
  "dependencies": {},
  "devDependencies": {
    "@typescript-eslint/parser": "^8.46.2"
  }
}
