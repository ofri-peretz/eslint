{
  "name": "eslint-plugin-nestjs-security",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/eslint-plugin-nestjs-security/src",
  "projectType": "library",
  "release": {
    "version": {
      "manifestRootsToUpdate": [
        "dist/{projectRoot}"
      ],
      "currentVersionResolver": "git-tag",
      "fallbackCurrentVersionResolver": "disk"
    }
  },
  "tags": [],
  "targets": {
    "build": {
      "executor": "@nx/js:tsc",
      "outputs": [
        "{options.outputPath}"
      ],
      "options": {
        "outputPath": "dist/packages/eslint-plugin-nestjs-security",
        "main": "packages/eslint-plugin-nestjs-security/src/index.ts",
        "tsConfig": "packages/eslint-plugin-nestjs-security/tsconfig.lib.json",
        "assets": [
          "packages/eslint-plugin-nestjs-security/*.md",
          "packages/eslint-plugin-nestjs-security/.npmignore"
        ]
      }
    },
    "test": {
      "executor": "@nx/vite:test",
      "outputs": [
        "{options.reportsDirectory}"
      ],
      "options": {
        "reportsDirectory": "../../coverage/packages/eslint-plugin-nestjs-security"
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "outputs": [
        "{options.outputFile}"
      ]
    },
    "nx-release-publish": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npm publish dist/{projectRoot} --access public --auth-type=web",
        "cwd": "{workspaceRoot}"
      }
    }
  }
}